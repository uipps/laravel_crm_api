{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "apache/thrift": "^0.13.0",
        "aws/aws-sdk-php-laravel": "^3.5",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "hhxsv5/laravel-s": "^3.7",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^2.0",
        "mews/captcha": "^3.0",
        "php-amqplib/php-amqplib": "^2.11",
        "predis/predis": "^1.1",
        "tucker-eric/eloquentfilter": "^2.3",
        "tymon/jwt-auth": "1.*@rc"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.7.0",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "laravel/telescope": "^3.2",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files":[
            "app/Libs/Utils/MyFunction.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer"
        }
    }
}
